<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
     
   <class name="com.nt.entity.ProgrammerProjectInfo"  table="Programmers_Projects">
    <!-- Composite Id Field cfg -->
       <composite-id name="id"  class="com.nt.entity.PrgmrProjId">
          <key-property name="projId"/>
          <key-property name="pid"/>
       </composite-id>
       <property name="pname"/>
       <property name="projName"/>
   </class>
   
</hibernate-mapping>	
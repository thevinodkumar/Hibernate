<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name="com.nt.entity.PhoneNumber"  table="PHONE_NUMBERS_MAP">
     <id name="phone"  type="long" length="10"/>
     <property name="numberType" type="string"  length="15"/>
     <property name="provider" type="string" length="10"/>
     <property name="unid"  type="int" insert="false" update="false" length="5"/>
   </class>
</hibernate-mapping>